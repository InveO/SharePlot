<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE DATABASE SYSTEM "database2.dtd">
<!-- 
    Warning : The Column name must match the name of the field in the java classes

-->
<DATABASE name="SharePlot" >
    <TABLE name="Portfolio" localname="Portfolio">
        <COLUMN name="idPortfolio" localname="idPortfolio" dbtype="INTEGER"/>
        <COLUMN name="isFake" localname="isFake" dbtype="CHAR"/>
        <COLUMN name="name" localname="name" dbtype="VARCHAR"/>
    </TABLE>
    <TABLE name="Share" localname="Share">
        <COLUMN name="idShare" localname="idShare" dbtype="INTEGER"/>
        <COLUMN name="codeISIN" localname="codeISIN" dbtype="VARCHAR"/>
        <COLUMN name="codeYahoo" localname="codeYahoo" dbtype="VARCHAR"/>
        <COLUMN name="description" localname="description" dbtype="VARCHAR"/>
        <COLUMN name="name" localname="name" dbtype="VARCHAR"/>
    </TABLE>
    <TABLE name="ShareQuantity" localname="ShareQuantity">
        <COLUMN name="idShareQuantity" localname="idShareQuantity" dbtype="INTEGER"/>
        <COLUMN name="changeFee" localname="changeFee" dbtype="DECIMAL"/>
        <COLUMN name="changeQuantity" localname="changeQuantity" dbtype="DECIMAL"/>
        <COLUMN name="changeType" localname="changeType" dbtype="CHAR"/>
        <COLUMN name="changeValue" localname="changeValue" dbtype="DECIMAL"/>
        <COLUMN name="description" localname="description" dbtype="VARCHAR"/>
        <COLUMN name="idPortfolio" localname="idPortfolio" dbtype="INTEGER"/>
        <COLUMN name="idShare" localname="idShare" dbtype="INTEGER"/>
        <COLUMN name="totalQuantity" localname="totalQuantity" dbtype="DECIMAL"/>
        <COLUMN name="valueDate" localname="valueDate" dbtype="DATE"/>
    </TABLE>
    <TABLE name="ShareValue" localname="ShareValue">
        <COLUMN name="idShareValue" localname="idShareValue" dbtype="INTEGER"/>
        <COLUMN name="idShare" localname="idShare" dbtype="INTEGER"/>
        <COLUMN name="value" localname="value" dbtype="DECIMAL"/>
        <COLUMN name="valueDate" localname="valueDate" dbtype="DATE"/>
    </TABLE>
