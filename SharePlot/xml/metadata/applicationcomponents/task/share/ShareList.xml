<APPLICATION_COMPONENT class="jet.framework.component.SimpleApplicationComponent" initialNutName="ShareListNut">

	<!-- SimpleApplicationComponent specific declarations -->
	<APPLICATION_COMPONENT_CONFIG>
	
		<SHARE_LIST>
<!--         	<COLUMN name="selectColumn" datamodelfield="" type="CHECKBOX" editable="true" width="0" /> -->
     	    <COLUMN name="nameColumn" datamodelfield="name" type="TEXT" editable="true" width="0" localizedlabel="SharePlot/properties/desktop/Global/list.name" />
			<COLUMN name="descrColumn" datamodelfield="description" type="TEXT" editable="true" width="10" localizedlabel="SharePlot/properties/desktop/Global/list.description" />
			<COLUMN name="codeColumn" datamodelfield="code" type="TEXT" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.code" />
