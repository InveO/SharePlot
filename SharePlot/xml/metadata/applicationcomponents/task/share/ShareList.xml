			<COLUMN name="priceColumn" datamodelfield="purchasePrice" type="NUMBER" class="java.lang.Double" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.purchasePrice" />
			<COLUMN name="feeColumn" datamodelfield="entryFee" type="NUMBER" class="java.lang.Double" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.entryFee" />
			<COLUMN name="editColumn" datamodelfield="" type="BUTTON" editable="true" width="0" style="tableColumnButton" localizedlabel="SharePlot/properties/task/Global/list.edit" />
		</SHARE_LIST>

	</APPLICATION_COMPONENT_CONFIG>

	<NUTS>
		<NUT name="ShareListNut" className="jet.shareplot.ui.task.share.ShareListNut">
			<PARAMETER name="formName" value="ShareListForm" />
			<PARAMETER name="formXmlFile" value="SharePlot/xml/ui/task/share/ShareListForm.xml" />
		</NUT>
	</NUTS>

</APPLICATION_COMPONENT>