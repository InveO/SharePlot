<APPLICATION_COMPONENT class="jet.framework.component.SimpleApplicationComponent" initialNutName="ShareQuantityListNut">

	<!-- SimpleApplicationComponent specific declarations -->
	<APPLICATION_COMPONENT_CONFIG>
	
		<QUANTITY_LIST>
        	<COLUMN name="colSelect" datamodelfield="" type="CHECKBOX" editable="true" width="0" selectall="true" />
			<COLUMN name="dateColumn" datamodelfield="valueDate" type="DATE" editable="true" width="1" localizedlabel="SharePlot/properties/task/Share/list.purchaseDate" />
     	    <COLUMN name="typeColumn" datamodelfield="changeType" type="TEXT" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.changeType" />
			<COLUMN name="priceColumn" datamodelfield="changeValue" type="NUMBER" class="java.math.BigDecimal" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.changePrice" />
			<COLUMN name="feeColumn" datamodelfield="changeFee" type="NUMBER" class="java.math.BigDecimal" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.changeFee" />
			<COLUMN name="quantityColumn" datamodelfield="changeQuantity" type="NUMBER" class="java.math.BigDecimal" editable="true" width="0" localizedlabel="SharePlot/properties/task/Share/list.changeQuantity" />
<!-- 			<COLUMN name="editColumn" datamodelfield="" type="BUTTON" editable="true" width="0" style="tableColumnButton" localizedlabel="SharePlot/properties/desktop/Global/list.edit" /> -->
		</QUANTITY_LIST>

	</APPLICATION_COMPONENT_CONFIG>

	<NUTS>
		<NUT name="ShareQuantityListNut" className="jet.shareplot.ui.task.share.ShareQuantityListNut">
			<PARAMETER name="formName" value="ShareQuantityListForm" />
			<PARAMETER name="formXmlFile" value="SharePlot/xml/ui/task/share/ShareQuantityListForm.xml" />
		</NUT>
	</NUTS>

</APPLICATION_COMPONENT>