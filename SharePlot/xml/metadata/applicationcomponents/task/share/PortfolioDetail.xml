<APPLICATION_COMPONENT class="jet.framework.component.SimpleApplicationComponent" initialNutName="PortfolioDetailNut">

    <!-- SimpleApplicationComponent specific declarations -->
    <APPLICATION_COMPONENT_CONFIG>

        <SHARE_LIST>
            <COLUMN name="colSelect" datamodelfield="" type="CHECKBOX" editable="true" width="0" selectall="true" />
            <COLUMN name="shareColumn" datamodelfield="" type="AUTOCOMPLETE" editable="true" width="10" localizedlabel="SharePlot/properties/task/Share/list.share" />
            <COLUMN name="dateColumn" datamodelfield="valueDate" type="DATE" editable="true" width="5" localizedlabel="SharePlot/properties/task/Share/list.valueDate" />
            <COLUMN name="quantityColumn" datamodelfield="changeQuantity" type="NUMBER" class="java.math.BigDecimal" editable="true" width="5" localizedlabel="SharePlot/properties/task/Share/list.quantity" />
            <COLUMN name="valueColumn" datamodelfield="value" type="NUMBER" class="java.math.BigDecimal" editable="false" width="5" localizedlabel="SharePlot/properties/task/Share/list.value" />
            <COLUMN name="editColumn" datamodelfield="" type="BUTTON" editable="true" width="0" style="tableColumnButton" localizedlabel="SharePlot/properties/desktop/Global/list.edit" />
        </SHARE_LIST>

    </APPLICATION_COMPONENT_CONFIG>

    <NUTS>
        <NUT name="PortfolioDetailNut" className="jet.shareplot.ui.task.share.PortfolioDetailNut">
            <PARAMETER name="formName" value="PortfolioDetailForm" />
            <PARAMETER name="formXmlFile" value="SharePlot/xml/ui/task/share/PortfolioDetailForm.xml" />
        </NUT>
    </NUTS>

</APPLICATION_COMPONENT>
