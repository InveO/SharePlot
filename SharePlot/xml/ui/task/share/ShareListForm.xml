<DOCUMENT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="http://jet.inveo.be/ui/JETComponentExtension.xsd" >

    <GENERIC name="SharePlot/xml/ui/GenericComponent.xml" />
    
    <GROUP name="ShareListForm" localizedDisplayableText="SharePlot/properties/task/Share/title.ShareList" scrollable="false" layoutmanager="gridbaglayout" backgroundColor="panelBackgroundColor" foregroundColor="panelForegroundColor">
        <GROUP name="topGroup" extends="GenericTitleGroup">
            <GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="1" weighty="0" />
            <ATTRIBUTE_PARAM component="titleLabel" name="localizedDisplayableText" value="SharePlot/properties/task/Share/title.ShareList" />
        </GROUP>
        <GROUP name="toolbarGroup" layoutmanager="gridbaglayout" >
            <BUTTON name="saveButton" extends="SaveButton" >
                <GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="NONE" insetleft="5" />
            </BUTTON>
            <BUTTON name="deleteButton" extends="DeleteButton" >
                <GRIDBAGCONSTRAINTS gridx="1" gridy="0" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="NONE" insetleft="5" />
            </BUTTON>
            <GRIDBAGCONSTRAINTS gridx="0" gridy="1" gridwidth="1" gridheight="1" weightx="1" weighty="0" fill="HORIZONTAL" insetleft="5" />
        </GROUP>
        <DISPLAY2 name="tableList" extends="EmptyTable" >
            <GRIDBAGCONSTRAINTS gridx="0" gridy="2" gridheight="1" gridwidth="1" weightx="1" weighty="1" insetleft="10" insetright="10" insettop="10" insetbottom="5" anchor="NORTH" fill="BOTH"/>
        </DISPLAY2>
		<GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="1" weighty="1" anchor="NORTH" fill="BOTH"/>
	</GROUP>
</DOCUMENT>
