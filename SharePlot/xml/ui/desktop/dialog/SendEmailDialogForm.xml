<DOCUMENT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://jet.inveo.be/ui/ http://jet.inveo.be/ui/JETComponentExtension.xsd"
    xmlns="http://jet.inveo.be/ui/">
    
    <GENERIC name="SharePlot/xml/ui/GenericComponent.xml" />
    
	<GROUP name="SendEmailDialogForm" scrollable="false" layoutmanager="gridbaglayout" backgroundColor="panelBackgroundColor" foregroundColor="panelForegroundColor" >
		<LABEL name="label" extends="DialogLabel" displayableText=" " preferredwidth="300" multiline="true" >
			<GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="BOTH" anchor="CENTER" insetleft="10" insetright="10" />
		</LABEL>
		<GROUP name="body">
			<LABEL name="labelTitle" extends="DialogLabel" localizedDisplayableText="SharePlot/properties/desktop/Dialog/label.Title" >
				<GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="BOTH" anchor="NORTHWEST" insetleft="10" insetright="10" />
			</LABEL>
			<LABEL name="title" extends="DialogLabel" >
				<GRIDBAGCONSTRAINTS gridx="1" gridy="0" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="BOTH" anchor="NORTHWEST" insetleft="10" insetright="10" />
			</LABEL>
			<LABEL name="labelEmailTo" extends="DialogLabel" localizedDisplayableText="SharePlot/properties/desktop/Dialog/label.EmailTo" >
				<GRIDBAGCONSTRAINTS gridx="0" gridy="1" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="BOTH" anchor="NORTHWEST" insetleft="10" insetright="10" />
			</LABEL>
			<TEXT name="emailTo" extends="TextFieldEditor" submit="true">
				<GRIDBAGCONSTRAINTS gridx="1" gridy="1" gridwidth="1" gridheight="1" weightx="1" weighty="0" fill="HORIZONTAL" anchor="NORTHWEST" insetleft="10" insetright="10" />
			</TEXT>
			<LABEL name="labelWarningEmail" extends="DialogBoldRedLabel" localizedDisplayableText="SharePlot/properties/desktop/Dialog/message.InvalidEmail" visible="false">
				<GRIDBAGCONSTRAINTS gridx="1" gridy="2" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="BOTH" anchor="NORTHWEST" insetleft="10" insetright="10" />
			</LABEL>
			<LABEL name="labelMessage" extends="DialogLabel" localizedDisplayableText="SharePlot/properties/desktop/Dialog/label.Message" >
				<GRIDBAGCONSTRAINTS gridx="0" gridy="3" gridwidth="1" gridheight="1" weightx="0" weighty="0" fill="BOTH" anchor="NORTHWEST" insetleft="10" insetright="10" />
			</LABEL>
			<TEXTAREA name="message" extends="TextAreaEditor" >
				<GRIDBAGCONSTRAINTS gridx="1" gridy="3" gridwidth="1" gridheight="1" weightx="1" weighty="1" fill="BOTH" anchor="NORTHWEST" insetleft="10" insetright="10" insetbottom="1"/>
	    	</TEXTAREA>
			<GRIDBAGCONSTRAINTS gridx="0" gridy="1" gridwidth="1" gridheight="1" weightx="1" weighty="1" fill="BOTH" anchor="CENTER" insetbottom="2" insetleft="2" insetright="2" />
		</GROUP>
		<GROUP name="buttons">
			<BUTTON name="sendButton" extends="GenericButton" >
				<GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="1" weighty="0" fill="NONE" anchor="CENTER" insetbottom="2" insetleft="2" insetright="2" insettop="2" />
			</BUTTON>
			<BUTTON name="cancelButton" extends="GenericButton" >
				<GRIDBAGCONSTRAINTS gridx="1" gridy="0" gridwidth="1" gridheight="1" weightx="1" weighty="0" fill="NONE" anchor="CENTER" insetbottom="2" insetleft="2" insetright="2" insettop="2" />
			</BUTTON>
			<GRIDBAGCONSTRAINTS gridx="0" gridy="2" gridwidth="1" gridheight="1" weightx="1" weighty="1" anchor="SOUTHWEST" fill="HORIZONTAL" />
		</GROUP>
        <GROUP name="statusBarGroup" extends="DialogStatusBarGroup" >
            <GRIDBAGCONSTRAINTS gridx="0" gridy="3" gridwidth="1" gridheight="1" weightx="1" weighty="1" anchor="SOUTHWEST" fill="HORIZONTAL" />
        </GROUP>
		<GRIDBAGCONSTRAINTS gridx="0" gridy="0" gridwidth="1" gridheight="1" weightx="1" weighty="1" anchor="NORTH" fill="BOTH"/>
	</GROUP>
</DOCUMENT>